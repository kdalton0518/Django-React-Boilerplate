{
  "name": "django-react-typescript-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/marcelovicentegc/django-react-typescript.git",
  "author": "Marcelo Cardoso <dev@marcelo.page>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "unused": "pnpm unimported",
    "build": "cross-env webpack --config webpack.config.js",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "dev": "cross-env webpack-dev-server --progress --host 0.0.0.0",
    "test:static": "tsc -p tsconfig.json --noEmit",
    "test": "jest \"(/__tests__/.)*\\.tsx?$\" --coverage --colors --silent",
    "test:watch": "jest \"(/__tests__/.)*\\.tsx?$\" --coverage --colors --watch"
  },
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "dayjs": "^1.8.29",
    "husky": "^4.2.5",
    "internal-ip": "^5.0.0",
    "query-string": "^6.13.0",
    "react-router-dom": "^6.23.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@types/node": "^20.14.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^3.6.0",
    "dotenv-webpack": "^8.1.0",
    "html-webpack-plugin": "^5.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "typescript": "^5.4.5",
    "unimported": "^1.31.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-manifest-plugin": "^5.0.0",
    "workbox-webpack-plugin": "^7.1.0"
  }
}
